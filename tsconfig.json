{
  "compilerOptions": {
    "types": [],
    "lib": [
      "ES2019",
      "ES2020.String",
      "DOM",
      "DOM.Iterable"
    ],
    "noImplicitAny": false,
    "noImplicitOverride": true,
    "strict": true,
    "module": "es6",
    "target": "es2019",
    "sourceMap": true,
    "allowJs": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src/**/*"]
}
